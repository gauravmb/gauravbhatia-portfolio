{
  "name": "functions",
  "version": "1.0.0",
  "description": "Firebase Functions for Portfolio Website",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest --watch",
    "test:run": "jest"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^7.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.0",
    "@types/supertest": "^6.0.2",
    "fast-check": "^3.17.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.4.0"
  },
  "private": true
}
